
{
  "name": "blood-cell-analyzer",
  "version": "1.0.0",
  "description": "Blood Cell Analyzer Desktop Application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.bloodcellanalyzer.app",
    "productName": "Blood Cell Analyzer",
    "files": [
      "main.js",
      "preload.js",
      "../dist/**/*"
    ],
    "extraResources": [
      {
        "from": "../model.h5",
        "to": "model.h5"
      },
      {
        "from": "../python",
        "to": "python"
      }
    ],
    "directories": {
      "output": "../electron-dist"
    },
    "mac": {
      "category": "public.app-category.medical"
    },
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^35.0.1",
    "electron-builder": "^25.1.8",
    "electron-devtools-installer": "^3.2.0",
    "concurrently": "^9.1.2"
  },
  "dependencies": {
    "axios": "^1.8.3",
    "path": "^0.12.7",
    "fs": "0.0.1-security"
  }
}
